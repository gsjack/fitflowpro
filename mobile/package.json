    "@react-navigation/native-stack": "^6.11.0",
    "@tanstack/react-query": "^5.28.0",
    "axios": "^1.6.8",
    "date-fns": "^4.1.0",
    "expo": "~54.0.12",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.9",
